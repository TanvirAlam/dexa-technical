    "@trpc/client": "^11.0.0",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "pg": "^8.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
